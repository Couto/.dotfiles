"*":
  "atom-ternjs":
    displayAboveSnippets: true
    excludeLowerPriorityProviders: true
    guess: false
    lint: false
    sort: false
    useSnippets: true
    useSnippetsAndFunction: true
  "autocomplete-emojis": {}
  build: {}
  core:
    disabledPackages: [
      "exception-reporting"
      "metrics"
      "keyboard-localization"
      "linter-jscs"
      "linter-flow"
      "project-manager"
    ]
    projectHome: "/Users/couto/Development"
    themes: [
      "one-dark-ui"
      "duotone-dark-syntax"
    ]
  editor:
    fontFamily: "\"Fira Code\""
    fontSize: 13
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
  emmet: {}
  "file-icons":
    onChanges: true
  "find-and-replace":
    openProjectFindResultsInRightPane: true
  fonts:
    fontFamily: "FiraCode"
  "fuzzy-finder": {}
  "git-history":
    showDiff: false
  "git-log": {}
  "git-plus": {}
  "highlight-line": {}
  "keyboard-localization":
    useKeyboardLayout: "pt_PT"
  linter:
    showErrorTabLine: true
    statusIconScope: "Line"
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
    globalNodePath: "/Users/couto/.nvm/versions/node/v5.7.1"
  "linter-jscs":
    esnext: true
    onlyConfig: true
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "markdown-preview":
    useGitHubStyle: true
  "merge-conflicts": {}
  minimap:
    plugins:
      "minimap-hide": true
      "minimap-hideDecorationsZIndex": 0
  pigments:
    mergeColorDuplicates: true
    supportedFiletypes: [
      ".css"
      ".scss"
      ".sass"
      ".less"
      ".styl"
      ".stylus"
    ]
  "project-manager":
    closeCurrent: true
  react: {}
  "spell-check": {}
  "split-diff":
    ignoreWhitespace: true
  "status-bar": {}
  "symbols-tree-view": {}
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  "terminal-plus":
    core: {}
    style: {}
  "tree-view":
    showOnRightSide: true
    squashDirectoryNames: true
  "vim-mode":
    useClipboardAsDefaultRegister: false
  "vim-surround": {}
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
